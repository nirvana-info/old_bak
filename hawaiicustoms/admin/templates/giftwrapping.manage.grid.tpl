<table class="GridPanel SortableGrid" cellspacing="0" cellpadding="0">
	<tr style="%%GLOBAL_HidePaging%%">
		<td colspan="6" style="text-align: right; padding: 0 0 6px 0;" class="PagingNav">
			%%GLOBAL_Nav%%
		</td>
	</tr>
	<tr class="Heading3">
		<td style="text-align: center; width: 10px;"><input type="checkbox" onclick="$(this.parentForm).find('input:checkbox').not(':disabled').attr('checked', this.checked);" /></td>
		<td style="width: 10px;">&nbsp;</td>
		<td>
			%%LNG_WrapName%%
			%%GLOBAL_SortLinksWrapName%%
		</td>
		<td style="width: 150px;">
			%%LNG_Price%%
			%%GLOBAL_SortLinksWrapPrice%%
		</td>
		<td style="width: 80px; text-align: center;">
			%%LNG_Visible%%
			%%GLOBAL_SortLinksWrapVisible%%
		</td>
		<td style="width: 160px;">%%LNG_Action%%</td>
	</tr>
	%%GLOBAL_GiftWrapGrid%%
	<tr>
		<td colspan="6" style="text-align: right; padding: 6px 0;" class="PagingNav">
			%%GLOBAL_Nav%%
		</td>
	</tr>
</table>