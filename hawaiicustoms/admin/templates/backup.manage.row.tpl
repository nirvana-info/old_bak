<tr class="GridRow" onmouseover="this.className='GridRowOver'" onmouseout="this.className='GridRow'">
	<td align="center" style="width:25px">
		<input type="checkbox" name="backup[]" class="DeleteBackup" value="%%GLOBAL_FileName%%">
	</td>
	<td align="center" style="width:18px;">
		<img src='images/%%GLOBAL_BackupImage%%.gif' title='%%GLOBAL_BackupType%%'>
	</td>
	<td>
		%%GLOBAL_FileName%%
	</td>
	<td align="right">
		%%GLOBAL_FileSize%%
	</td>
	<td>
		%%GLOBAL_ModifiedTime%%
	</td>
	<td>
		<a title='%%GLOBAL_DownloadOpen%%' class='Action' href='%%GLOBAL_ViewLink%%' target='_blank'>%%GLOBAL_DownloadOpen%%</a>
	</td>
</tr>