<tr id="tr%%GLOBAL_ExportTemplateId%%" class="GridRow" onmouseover="this.className='GridRowOver'" onmouseout="this.className='GridRow'">
	<td align="center" style="width: 25px; %%GLOBAL_HideCheckAll%%">
		%%GLOBAL_CheckTemplate%%
	</td>
	<td align="center" style="width:18px">
		<img src="images/application_form.png" width="12" height="15" />
	</td>
	<td colspan="%%GLOBAL_ProductNameSpan%%" class="%%GLOBAL_SortedFieldNameClass%%">
		%%GLOBAL_ExportTemplateName%%
	</td>
	<td>
		%%GLOBAL_TemplateType%%
	</td>
	<td %%GLOBAL_HideVendorColumn%%>
		%%GLOBAL_VendorName%%
	</td>
	<td>
		<select id="select%%GLOBAL_ExportTemplateId%%" style="width: 160px;" onchange="PerformAction(this);">
			<option value="">%%LNG_ChooseAnAction%%</option>
			%%GLOBAL_TemplateActions%%
		</select>
	</td>
</tr>