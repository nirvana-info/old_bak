
	<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" >
	<HTML>
		<HEAD>
			<TITLE>%%LNG_PreviewReview%%</TITLE>
			<LINK href="Styles/windowstyles.css" type="text/css" rel="stylesheet">
		</HEAD>
		<BODY>
			<div class='Bar'>%%LNG_PreviewReview%%
				(<A href="javascript:window.close()">%%LNG_CloseWindow%%</A>)
			</div>
			<table id="Table" class="BodyContainer">
				<tr>
					<td class="Heading">
						%%GLOBAL_Title%%
						<br />%%GLOBAL_Rating%%
					</td>
				</tr>
				<tr>
					<td class="Heading">
						<hr size="1" noshade>
						Detail Ratings
					</td>
				</tr>
				<tr>
					<td class="Intro">
						<table border='0' width='90%'>
							<tr>
								<td >%%LNG_RateProductQuality%%:</td>
								<td>%%GLOBAL_RatingQuality%%</td>
							</tr>
							<tr>
								<td>%%LNG_RateEaseInstallation%%:</td>
								<td>%%GLOBAL_RatingInstall%%</td>
							</tr>
							<tr>
								<td>%%LNG_RateProductValue%%:</td>
								<td>%%GLOBAL_RatingValue%%</td>
							</tr>
							<tr>
								<td>%%LNG_RateCustomerSupport%%:</td>
								<td>%%GLOBAL_RatingSupport%%</td>
							</tr>
							<tr>
								<td>%%LNG_RateDeliveryTime%%:</td>
								<td>%%GLOBAL_RatingDelivery%%</td>
							</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td class="Intro">
						<hr size="1" noshade>
						<table border='0' width='95%'>
							<tr>
								<td width='60'><strong>Year</strong></td>
								<td width='120'><strong>Make</strong></td>
								<td><strong>Model</strong></td>
							</tr>
							<tr>
								<td>%%GLOBAL_RateYear%%</td>
								<td>%%GLOBAL_RateMake%%</td>
								<td>%%GLOBAL_RateModel%%</td>
							</tr>
						</table>
					</td>
				</tr>
				<tr>
					<td class="Intro">
						<hr size="1" noshade>
						<strong>%%LNG_Product%%: %%GLOBAL_Product%%</strong>
						<br /><strong>%%LNG_PostedBy%%: %%GLOBAL_Author%%</strong>
						<hr size="1" noshade>
					</td>
				</tr>
				<tr>
					<td class="Intro">
						%%GLOBAL_Review%%
						<br />
					</td>
				</tr>
			</table>
		</BODY>
	</HTML>