<table id="OuterPanel" cellSpacing="0" cellPadding="0" width="95%">
	<tr>
		<td class="Heading1">
			%%GLOBAL_ReportTitle%%
		</td>
	</tr>
	<tr>
		<td class="Intro">
			<div>%%GLOBAL_ReportDesc%%</div>
		</td>
	</tr>
	<tr>
		<td class="Intro">
			%%GLOBAL_Report%%
			<p style="text-align: center;"><input type="button" value="Close" class="SmallButton" onclick="self.parent.tb_remove()" /></p>
		</td>
	</tr>
</table>
<script type="text/javascript">
	function ShowReport(reporttype) {
		var link = 'index.php?ToDo=viewConverterLog&module=%%GLOBAL_Module%%&log='+reporttype;
		var top = screen.height / 2 - (220);
		var left = screen.width / 2 - (250);
		window.open(link,"reportWin","left=" + left + ",top="+top+",toolbar=false,status=no,directories=false,menubar=false,scrollbars=false,resizable=false,copyhistory=false,width=500,height=440");
	}
</script>