<tr id="tr%%GLOBAL_ProductId%%" class="GridRow" onmouseover="this.className='GridRowOver'" onmouseout="this.className='GridRow'">
	<td align="center" style="width:25px">
		<input type="checkbox" name="products[]" value="%%GLOBAL_ProductId%%">
	</td>
	<td align="center" style="width:15px; display: %%GLOBAL_HideInventoryOptions%%">
		%%GLOBAL_StockExpand%%
	</td>
	<td align="center" style="width:20px">
		<img src="images/product.gif" alt="product" height="16" width="16" />
	</td>
	<td style="width:50px" nowrap class="ImageField" align="center">
		%%GLOBAL_ProductImage%%
	</td>
	<td style="width:100px" class="%%GLOBAL_SortedFieldCodeClass%%">
		%%GLOBAL_SKU%%
	</td>
	<td id="InStock%%GLOBAL_ProductId%%" class="%%GLOBAL_SortedFieldStockClass%% %%GLOBAL_LowStockStyle%%" style="display: %%GLOBAL_HideInventoryOptions%%">
		%%GLOBAL_StockInfo%%
	</td>
	<td colspan="%%GLOBAL_ProductNameSpan%%" class="%%GLOBAL_SortedFieldNameClass%%">
		%%GLOBAL_Name%%
	</td>
	<td style="text-align: right;" class="%%GLOBAL_SortedFieldPriceClass%%">
		%%GLOBAL_Price%%
	</td>
	<td align="center" class="%%GLOBAL_SortedFieldVisibleClass%%">
		%%GLOBAL_Visible%%
	</td>
	<td align="center" class="%%GLOBAL_SortedFieldFeaturedClass%%">
		%%GLOBAL_Featured%%
	</td>
	<td>
		%%GLOBAL_EditProductLink%%
		%%GLOBAL_CopyProductLink%%
	</td>
</tr>
<tr id="trQ%%GLOBAL_ProductId%%" style="display:none">
	<td colspan="6">
		&nbsp;
	</td>
	<td colspan="2" class="ProductQuickView QuickView" id="tdQ%%GLOBAL_ProductId%%">
	</td>
	<td colspan="3">
		&nbsp;
	</td>
</tr>