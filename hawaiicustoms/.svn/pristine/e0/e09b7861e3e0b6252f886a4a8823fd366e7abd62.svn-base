
	<tr class="GridRow" onmouseover="this.className='GridRowOver'" onmouseout="this.className='GridRow'">
		<td align="center" style="width:23px">
			<input type="checkbox" name="users[]" value="%%GLOBAL_UserId%%" %%GLOBAL_CheckDisabled%%>
		</td>
		<td align="center" style="width:18px;">
			<img src="images/user.gif" />
		</td>
		<td class="%%GLOBAL_SortedFieldUserClass%%">
			%%GLOBAL_Username%%
		</td>
		<td class="%%GLOBAL_SortedFieldNameClass%%">
			%%GLOBAL_Name%%
		</td>
		<td class="%%GLOBAL_SortedFieldEmailClass%%">
			%%GLOBAL_Email%%
		</td>
		<td style="width: 150px; %%GLOBAL_HideVendorColumn%%" class="%%GLOBAL_SortedFieldVendorClass%%">
			%%GLOBAL_Vendor%%
		</td>
		<td align="center" style="display: %%GLOBAL_StatusField%%" class="%%GLOBAL_SortedFieldStatusClass%%">
			%%GLOBAL_Status%%
		</td>
		<td>
			%%GLOBAL_EditUserLink%%&nbsp;&nbsp;&nbsp;
			%%GLOBAL_CopyUserLink%%
		</td>
	</tr>
