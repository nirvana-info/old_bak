<tr class="GridRow" onmouseover="this.className='GridRowOver'" onmouseout="this.className='GridRow'">
	<td align="center" style="width:18px;">
		<img src='images/brand.gif' width="15" height="15">
	</td>
	<td class="%%GLOBAL_SortedFieldBrandClass%%">
		%%GLOBAL_Year%%
	</td>
    <td class="%%GLOBAL_SortedFieldBrandClass%%">
        %%GLOBAL_Make%%
    </td>
    <td class="%%GLOBAL_SortedFieldBrandClass%%">
        %%GLOBAL_Model%%
    </td>
    <td class="%%GLOBAL_SortedFieldBrandClass%%">
        %%GLOBAL_SubModel%%
    </td>
    <td>
        %%GLOBAL_EngineType%%    
    </td>
    <td>
        %%GLOBAL_Liter%%    
    </td>
	<td style="display: %%GLOBAL_DisplayAction%%">
		%%GLOBAL_EditEngineMMYLink%% | %%GLOBAL_DeleteEngineMMYLink%%
	</td>
</tr>