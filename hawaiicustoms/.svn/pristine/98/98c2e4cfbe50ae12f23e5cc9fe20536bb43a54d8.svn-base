<link rel="stylesheet" href="Styles/invoicestyles.css" type="text/css">
	<table class="MainTable" width="100%" cellspacing="5" cellpadding="5">
	<tr>
		<td colspan="2" class="InvoiceTitle">
			%%GLOBAL_StoreName%% %%GLOBAL_PackingSlipTitle%%
		</td>
	</tr>
	<tr>
		<td colspan="2" class="Heading2">
			%%GLOBAL_StoreAddressFormatted%%
		</td>
	</tr>

	<tr>
		<td colspan="2"  class="Heading3">
			%%LNG_CustomerDetails%%
		</td>
	</tr>
	<tr>
		<td colspan="2">
			%%GLOBAL_CustomerDetails%%
		</td>
	</tr>
	<tr>
		<td width="50%" class="Heading3">
			%%LNG_BillTo%%
		</td>
		<td width="50%" class="Heading3">
			%%LNG_ShipTo%%
		</td>
	</tr>
	<tr>
		<td width="50%" valign="top">
			%%GLOBAL_BillingAddress%%
		</td>
		<td width="50%" valign="top">
			%%GLOBAL_ShippingAddress%%
		</td>
	</tr>
	<tr>
		<td width="50%" valign="top">
			<table cellspacing="0" cellpadding="0">
				<tr>
					<td style="padding-bottom: 3px; padding-right: 10px; font-weight: bold;">%%LNG_Order%%:</td>
					<td style="padding-bottom: 3px;">#%%GLOBAL_OrderId%%</td>
				</tr>
				<tr>
					<td style="padding-bottom: 3px; padding-right: 10px; font-weight: bold;">%%LNG_ShipmentOrderDate%%:</td>
					<td style="padding-bottom: 3px;">%%GLOBAL_OrderDate%%</td>
				</tr>
			</table>
		</td>
		<td width="50%" valign="top">
			<table cellspacing="0" cellpadding="0">
				<tr style="%%GLOBAL_HideShippingMethod%%">
					<td style="padding-bottom: 3px; padding-right: 10px; font-weight: bold;">%%LNG_ShippingMethod%%:</td>
					<td style="padding-bottom: 3px;">%%GLOBAL_ShippingMethod%%</td>
				</tr>
				<tr style="%%GLOBAL_HideShippingDate%%">
					<td style="padding-bottom: 3px; padding-right: 10px; font-weight: bold;">%%LNG_DateShipped%%:</td>
					<td style="padding-bottom: 3px;">%%GLOBAL_DateShipped%%</td>
				</tr>
				<tr style="%%GLOBAL_HideTrackingNo%%">
					<td style="padding-bottom: 3px; padding-right: 10px; font-weight: bold;">%%LNG_TrackingNumber%%:</td>
					<td style="padding-bottom: 3px;">%%GLOBAL_TrackingNo%%</td>
				</tr>
			</table>
		</td>
	</tr>
	<tr>
		<td colspan="2" class="Heading3">
			<hr size="1" noshade>%%LNG_ShippedItems%%
		</td>
	</tr>
	<tr>
		<td colspan="2">
			<table width="95%" border="0" align="right">
				<tr>
					<td class="Heading2" width="150">%%LNG_Quantity%%</td>
					<td class="Heading2" width="200">%%LNG_Code%%</td>
					<td class="Heading2">%%LNG_ProdName%%</td>
				</tr>
				%%GLOBAL_ProductsTable%%
			</table>
		</td>
	</tr>
	<tr style="%%GLOBAL_HideComments%%">
		<td colspan="2" class="Heading3">
			<hr size="1" noshade="noshade" />
			%%LNG_Comments%%
		</td>
	</tr>
	<tr style="%%GLOBAL_HideComments%%">
		<td colspan="2">
			<blockquote>%%GLOBAL_Comments%%</blockquote>
		</td>
	</tr>
</table>