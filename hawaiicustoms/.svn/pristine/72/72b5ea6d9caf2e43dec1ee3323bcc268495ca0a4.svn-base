return false;
}

if(seostatus != 0)
{
var str = search_query.value;
//str = str.replace(/ /g,"-");
str = str.toLowerCase();
str = MakeURLSafe(str);
location.href = "http://www.truckchamp.com/search/"+str;
return false;
}
else
{
return true;
}
}

