<li id="ele-%%GLOBAL_FieldID%%" class="SortableRow%%GLOBAL_TypeName%% SortableRow" style="margin-left: 0px;">
	<table class="GridPanel" cellspacing="0" cellpadding="0" border="0" style="width:100%;">
		<tr class="GridRow" id="row_%%GLOBAL_FieldID%%" onmouseover="this.className='GridRowOver'" onmouseout="this.className='GridRow'">
			<td align="center" style="width: %%GLOBAL_CheckColWidth%%px; %%GLOBAL_CheckAlign%%">
				%%GLOBAL_NodeJoin%%
				<input type="checkbox" name="%%GLOBAL_FieldType%%Field[%%GLOBAL_FieldID%%]" id="%%GLOBAL_FieldID%%" %%GLOBAL_FieldChecked%% value="1" />
			</td>
			<td class="DragMouseDown sort-handle %%GLOBAL_FieldLabelClass%%" style="width: 155px; %%GLOBAL_CheckAlign%%" id="label_%%GLOBAL_FieldID%%">
				%%GLOBAL_FieldLabel%%:
			</td>
			<td id="headercol_%%GLOBAL_FieldID%%">
				<input type="text" class="Field200 %%GLOBAL_FieldClass%%" id="header_%%GLOBAL_FieldID%%" name="%%GLOBAL_FieldType%%Header[%%GLOBAL_FieldID%%]" value="%%GLOBAL_FieldHeader%%" maxlength="63" %%GLOBAL_FieldReadOnly%%/>
				%%GLOBAL_FieldHelp%%
				%%GLOBAL_SubFields%%
			</td>
		</tr>
	</table>
</li>