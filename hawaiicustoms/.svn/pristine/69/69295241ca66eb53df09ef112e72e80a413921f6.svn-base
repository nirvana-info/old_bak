								<form action="#" id="LoginForm" method="post" onsubmit="ExpressCheckout.Login(); return false;">
									<div class="FormContainer HorizontalFormContainer NarrowFormContainer">
										<p style="display: none" class="ErrorMessage" id="CheckoutLoginError">
											%%GLOBAL_LoginMessage%%
										</p>

										<dl>
											<dt>%%LNG_EmailAddress%%:</dt>
											<dd style="margin-left:110px;"><input type="text" class="Textbox Field150 InitialFocus" name="login_email" id="login_email" /></dd>

											<dt>%%LNG_Password%%:</dt>
											<dd style="margin-left:110px;"><input type="password" class="Textbox Field150" name="login_pass" id="login_pass" /></dd>
										</dl>
										<p class="Submit"  style="margin-top:2px;">
											<input id="LoginButton" type="submit" value="%%LNG_Continue%%" style="padding:2px 10px 2px 10px" /> <span class="LoadingIndicator" style="display: none"><img src="%%GLOBAL_TPL_PATH%%/images/Loading.gif" alt="" /></span>
											<br />
											<a href="%%GLOBAL_ShopPath%%/login.php?action=reset_password">%%LNG_ForgotYourPassword%%</a>
										</p>
									</div>
								</form>
							</div>

							<div class="ECGoogleCheckoutButton">
								<h4>%%LNG_AlternateCheckout%%</h4>
								%%GLOBAL_AdditionalCheckoutButtons%%
							</div>

						</div>
					</div>

					<div class="Clear ExpressCheckoutBlock %%GLOBAL_CollapsedStepClassBillingAddress%%" id="CheckoutStepBillingAddress">
