%%Panel.HTMLHead%%
<body style="height: auto;">
<div id="LeftCol">
<div id="RightCol">
	<script type="text/javascript" src="%%GLOBAL_AppPath%%/javascript/express.checkout.js"></script>
	<div id="Container">
		%%Panel.Header%%
		%%Panel.FreeDelivery%%
		%%Banner.TopBanner%%
		<div id="Wrapper" style="background-color:#FFFFFF;">
			%%Panel.WrapperBanner%%
			<div class="Content Widest" id="LayoutColumn1">
				<div class="Block ExpressCheckout">
					%%GLOBAL_MakeAnOfferMsg%%
					<table width="100%" border="0" cellpadding="0" cellspacing="0">
						<tr>
							<td align="left" width="25%" valign="middle">
								<img border="0" alt="We Accept" src="%%GLOBAL_ShopPath%%/images/we-accept.jpg"/>
							</td>
							<td width="50%" align="center" valign="middle">
								<script type="text/javascript">
								// Dummy JS object to hold language strings before overwriting from the below verisign script.
								var temp_lang = {
								};
								temp_lang = lang;
								</script>
								<script>
									var dn="WWW.TRUCKCHAMP.COM";
									var lang="en";
									var aff="VeriSignCACenter";
									var tpt="transparent";
									var vrsn_style="WW";
									var splash_url="https://sealinfo.verisign.com";
									var seal_url="https://seal.verisign.com";
									var u1=splash_url+"/splash?form_file=fdf/splash.fdf&dn="+dn+"&lang="+lang;
									
									var sopener;
									function vrsn_splash() {
									 if (sopener && !sopener.closed){
									  sopener.focus();
									 } else {
									  tbar = "location=yes,status=yes,resizable=yes,scrollbars=yes,width=560,height=500";
									  var sw = window.open(u1,'VRSN_Splash',tbar);
									  if(sw) {
									   sw.focus();
									   sopener=sw;
									  }
									 }
									}
								</script>
								<script type="text/javascript">
								//lang JS object to revert it back from temp language strings to original lang variable.
								lang = temp_lang;
								</script>
								<a id="bigVeriSign" href="javascript:vrsn_splash()" style="color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;">
									<img border="0" src="%%GLOBAL_ShopPath%%/images/checkout-banner.jpg" alt="ABOUT SSL CERTIFICATES"/>
								</a>
							</td>
							<td align="right" width="25%" valign="middle">
								<a rel="nofollow" target="_blank"  href="http://www.bbb.org/northernindiana/business-reviews/truck-accessories-and-supplies/l-o-f-inc-dba-truckchamp-in-elkhart-in-90004817">
									<img border="0" alt="" src="%%GLOBAL_ShopPath%%/templates/TruckChampFixed/images/bbb-aplus.jpg"/>
								</a>
								<a href="http://www.bbb.org/northernindiana/business-reviews/truck-accessories-and-supplies/l-o-f-inc-dba-truckchamp-in-elkhart-in-90004817" rel="nofollow" target="_blank">
									<img border="0" width="53" height="84" oncontextmenu="alert('Use without permission is prohibited. The BBB Accredited Business seal is a trademark of the Council of Better Business Bureaus, Inc.'); return false;" title="Click to verify BBB accreditation and to see a BBB report." alt="Click to verify BBB accreditation and to see a BBB report." src="%%GLOBAL_ShopPath%%/images/tc-footer/bbbseal1.gif"/>
								</a>
								<a rel="nofollow" href="http://sema.org/Main/StartPage.aspx">
									<img border="0" width="61" height="83" alt="SEMA" src="%%GLOBAL_ShopPath%%/images/tc-footer/sema.gif"/>
								</a>
							</td>
						</tr>
					</table>
					<script type="text/javascript" src="%%GLOBAL_ShopPath%%/javascript/formfield.js"></script>
					<script type="text/javascript">
					lang.LoginEnterValidEmail = "%%LNG_LoginEnterValidEmail%%";
					lang.LoginEnterPassword = "%%LNG_LoginEnterPassword%%";
					lang.AccountEnterPassword = "%%LNG_AccountEnterPassword%%";
					lang.AccountPasswordsDontMatch = "%%LNG_AccountPasswordsDontMatch%%";
