* {
	margin: 0;
	padding: 0;
}

/* General */
html { 
	height: 101%; 
	font-size: 100%;
	margin-bottom: 1px;
	}
body {
	font-family: "Trebuchet MS", Helvetica, sans-serif;
	font-size: .95em;
	height: 100%;
	width: 100%;
	background: #ccc;
}

input, select {
	font-size: 11px;
}


p {
	line-height: 1.3;
	margin-bottom: 15px;
}

a {
	font-weight: normal;
	text-decoration: underline;
}

h2 {
	font-size: 1.4em;
	margin: 0 0 8px 0;
}

h3 {
	font-size: 1.1em;
	margin-bottom: 6px;
}

h4 {
	font-size: 1.1em;
	margin-bottom: 4px;
}

img, img a {
	border: 0;
}

ul, ol {
	margin: 0 0 10px 30px;
}

li {
	margin: 0;
	margin-bottom: 2px;
	padding: 0;
}

blockquote {
	margin-left: 28px;
}

select option {
	padding: 0 10px;
}

hr {
	margin: 10px 0;
	padding: 0;
	background: #DCDFE2;
	color: #DCDFE2;
	height: 1px;
	border:0;
}

/* Forms */
.HorizontalFormContainer dl, .VerticalFormContainer dl {
	margin: 0;
}

.HorizontalFormContainer dt {
	width: 160px;
	float: left;
	margin-bottom: 6px;
	clear: left;
}

.HorizontalFormContainer dd {
	margin-left: 170px;
	margin-bottom: 6px;
}

.NarrowFormContainer dt {
	width: 110px;
}

.NarrowFormContainer dd {
	margin-left: 120px;
}

.HorizontalFormContainer p.Submit {
	margin-left: 170px;
}

.NarrowFormContainer p.Submit {
	margin-left: 120px;
}

.VerticalFormContainer dt {
	margin-bottom: 6px;
}

.VerticalFormContainer dd {
	margin-bottom: 6px;
	margin-left: 10px;
}

/* Page Container */

#Container {
	width: 100%;
	min-width: 980px;
	max-width: 1200px;
	min-height: 100%;
    margin: 5px auto;
	text-align: center;
}
* html #container {
height: 100%; /* 100% height for ie6 */
}
/* Top Navigational Menu */
#TopMenu {
	width: 375px;
	/* width: 100%; */
	height: 23px;
	overflow: hidden;
	float: right;
	margin-top: 1px;
}

#TopMenu ul {
}

#TopMenu li {
	list-style: none;
	float: left;
	height: 23px;
	margin-left: -1px;
}

#TopMenu li.First  {
	padding: 0 0 0 0px;

}

#TopMenu li a, #TopMenu li div {
	float: left;
	display: inline;
	height: 23px;
	display: block;
	text-decoration: none;
	font-size: 0.85em;
	padding-right: 8px;
	margin: 0 0 7px 0;
	background: url(../../../images/tp.gif) top right no-repeat;
	color: #fff;

}
#TopMenu li a span {
	background: url(../../../images/tp-span.gif) left top no-repeat;
	display: block;
	height: 23px;
	padding-left: 5px;
}
#TopMenu li div a {
	padding: 0;
	margin: 0;
	display: inline;
	font-size: 1em;
}

#TopMenu li a:hover {
	border-bottom: 1px solid #fff;
}

