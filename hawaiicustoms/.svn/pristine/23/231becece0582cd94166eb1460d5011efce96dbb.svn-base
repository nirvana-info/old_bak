} 
#Menu ul {
	list-style: none;
	padding: 0;
	margin: 0px 0px 0 20px;
}

#Menu li {
	list-style: none;
	float: left;
	display: inline;
	height: 22px;
	padding: 0 0px 0 0px;
}

#Menu li.First {
		padding: 0 0 0 13px;
}

#Menu li a {
	display: block;
	float: left;
	font-size: .85em;
	text-decoration: none;
	padding: 2px 5px 3px 5px;
	height: 16px;
}

#Menu li a:hover	{
	text-decoration: underline;
}

#Menu li ul {
	width: auto;
	position: absolute;
	clear: left;
	top: 24px;
	left: -6px;
	display: none;
	text-align: left;
	font-size: 0.85em;
	background: transparent;
	z-index: 10;
}

#Menu li ul li {
	clear: both;
	text-align: left;
	background: transparent;
}

#Menu li ul li a {
	padding: 5px 5px 5px 5px;
	font-size: 1em;
	text-align: left;
	margin: 0;
	width: 120px;
	height: auto;
}

#Menu li ul li a:hover {
}

#Menu li:hover a, #Menu li.over a{
	text-decoration: underline;
}

#Menu li ul li a, #Menu li.over li a {
	text-decoration: none !important;
}

#Menu li:hover ul, #Menu li.over ul {
	display: block;
}

