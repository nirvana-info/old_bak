<table class="SubcatProdItem" cellpadding="0" cellspacing="0">    
    <tr class='%%GLOBAL_RowColor%%'>
		<td class="prodimage"  align='center'>
			<a href="%%GLOBAL_ProductURL%%" onmouseout = "hideTip();" onmouseover= "loadHoverImage(event, '', '%%GLOBAL_ProductDescription%%');"><img src = '%%GLOBAL_ProdImage%%' /><br>%%GLOBAL_ProdCode%%</a>
		</td>
        <td align="left"  valign="top" style="padding:8px 0px 0px 8px" class="prodqualifiers"> 
			%%GLOBAL_VehicleOptions%%
			%%GLOBAL_ProdOptions%%
        </td>
		<td class="prodbrandseries" align="center">%%GLOBAL_BrandName%%</td>
		<td class="prodpricedetails" align='center'>
			%%GLOBAL_ProductPrice%%
			<a href="%%GLOBAL_ProductURL%%">%%GLOBAL_ProductAddText%%</a>
			<a href="%%GLOBAL_CartURL%%">Add to Cart</a>
			<span style="display: %%GLOBAL_HideOfferButton%%;">
				 <a href='/makeaoffer.php?action=add&product_id=%%GLOBAL_ProductId%%' >Make an Offer</a>
			</span>
        </td>
    </tr>
</table>
